import numpy as np
from sklearn.linear_model import LinearRegression

# cosine of angle
X = np.array([ np.cos(.711), np.cos(-.063), np.cos(1.119), np.cos(-.248), np.cos(.497)]).reshape(-1, 1)
# y = 1 * x_0 + 2 * x_1 + 3
y = [1.3, 1.58, .90, 1.54, 1.53]
reg = LinearRegression().fit(X, y)
reg.score(X, y)

print(reg.coef_)