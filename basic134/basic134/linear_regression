import numpy as np
from sklearn.linear_model import LinearRegression

# cosine of angle
X = np.array([ np.cos(-.149), np.cos(.33), np.cos(.625), np.cos(.129), np.cos(.93)]).reshape(-1, 1)
# y = 1 * x_0 + 2 * x_1 + 3
y = [1.56, 1.63, 1.42, 1.84, 1.07]
reg = LinearRegression().fit(X, y)
reg.score(X, y)

print(reg.coef_)